{
    "name": "jwage/purl",
    "type": "library",
    "description": "URL Manipulation for PHP",
    "keywords": ["url", "manipulation"],
    "homepage": "http://github.com/jwage/purl",
    "license": "MIT",
    "authors": [
        {"name": "Jonathan H. Wage", "email": "jonwage@gmail.com"}
    ],
    "autoload": {
        "psr-4": {"Purl\\": "src/Purl"}
    },
    "autoload-dev": {
        "psr-4": {"Purl\\Test\\": "tests/Purl/Test"}
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "require": {
        "php": ">=8.1"
    },
    "require-dev": {
        "doctrine/coding-standard": "^8.0",
        "phpstan/phpstan-strict-rules": "^1.4",
        "phpunit/phpunit": "^8.1"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
